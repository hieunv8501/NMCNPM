
@{
    ViewBag.Title = "SVmodule2";
    Layout = "~/Areas/SV/Views/Shared/SVLayout.cshtml";
}

<html>
<head>
    <meta name="viewport" content="width=device-width" charset="utf-8" />
    <title>Trang chủ sinh viên</title>
</head>

<body>
    <div>
        <div>
            <a class="h2" style="float:right; color:blue; font-weight:700; font-family:Cambria ; margin-top:0; float:right">TRANG CHỦ SINH VIÊN</a>
            <img src="~/Content/img/forum uit sinhvien.png" style="width:100%; height:100%;" />
        </div>
    </div>
    <br />
    <div>
        <span class="generalStyle text-dark font-weight-bold">
            Chào mừng bạn đến với trang chủ của mình! Sinh viên @ViewData["TenDangNhap"]
        </span>
    </div>
    <br />
    <div>
        <h3 class="generalStyle text-danger font-weight-bold">BIỂU ĐỒ KẾ HOẠCH ĐÀO TẠO</h3>
        <br />
        <img style="width: 1230px; height: 800px" src="~/Content/img/2020-2021.jpg" alt="Biểu đồ kế hoạch năm 2020-2021" role="presentation">
        <img style="width: 1230px; height: 800px" src="~/Content/img/2019-2020.jpg" alt="Biểu đồ kế hoạch năm 2019-2020" role="presentation">
        <img style="width: 1230px; height: 800px" src="~/Content/img/2018-2019.png" alt="Biểu đồ kế hoạch năm 2018-2019" role="presentation">
    </div>
</body>
</html>